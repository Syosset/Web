.d-flex.justify-content-between
  %h1 Closures
  = render partial: 'partials/modtools', locals: {actions: @actions}

%table.table
  %thead
    %tr
      %th Start Date
      %th Type
  %tbody
    - @closures.each do |closure|
      = content_tag_for(:tr, closure) do
        %td= link_to closure.start_date, closure
        %td= closure.type
